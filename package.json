{
  "name": "destyle",
  "version": "0.0.3",
  "description": "Decouple your react component logic from your style logic",
  "main": "lib/destyle.js",
  "scripts": {
    "clean": "rimraf lib dist es",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/destyle.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/destyle.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/destyle.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/destyle.min.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syntropy-ai/destyle.git"
  },
  "keywords": [
    "react",
    "css",
    "style",
    "decouple"
  ],
  "author": "Syntropy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syntropy-ai/destyle/issues"
  },
  "homepage": "https://github.com/syntropy-ai/destyle#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "react": "^16.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1"
  }
}
